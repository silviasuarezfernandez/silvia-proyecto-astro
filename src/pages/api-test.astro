---


type Post = {
  id: number;
  title: string;
  body: string;
};

const response = await fetch('https://jsonplaceholder.typicode.com/posts');
const posts: Post[] = await response.json();
const primerosCinco = posts.slice(0, 5);
---

<h1>API Test</h1>
<ul>
  {primerosCinco.map(post => (
    <li>
      <h2>{post.title}</h2>
      <p>{post.body}</p>
    </li>
  ))}
</ul>

